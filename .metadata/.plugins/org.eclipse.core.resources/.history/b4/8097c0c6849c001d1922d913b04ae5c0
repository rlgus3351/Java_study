package test2;

import java.util.Scanner;

public class Ex18 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("정수 입력 : ");
		int num = sc.nextInt();

	}

	public static int Power(int base, int n) {
		int result = base;
		result *= base * n;
		return result;
	}

}
