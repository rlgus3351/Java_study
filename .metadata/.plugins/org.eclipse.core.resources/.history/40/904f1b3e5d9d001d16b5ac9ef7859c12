package test2;

public class Ex28 {

	public static void main(String[] args) {
		int cnt =1;
		int[][] array = new int[5][5];
		0403020100
		for(int i=0; i<array.length; i++) {
			for(int j=0; j<array[i].length; j++) {
				array[i][array.length-1-j] = cnt;
				cnt++;
			}
		}

		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array.length; j++) {
				System.out.print(array[i][j]+"\t");
			}
			System.out.println();
		}
	}

}
