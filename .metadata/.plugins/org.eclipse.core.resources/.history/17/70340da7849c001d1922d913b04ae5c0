package test2;

import java.util.Arrays;
import java.util.Scanner;

public class Ex18 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("정수 입력 : ");
		String text = sc.next();

		String[] temp = text.split("");
		System.out.println(Arrays.toString(temp));
	}

}
