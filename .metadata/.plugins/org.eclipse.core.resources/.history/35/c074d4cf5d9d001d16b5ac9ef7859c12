package test2;

import java.util.Scanner;

public class Ex29 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("N 입력");
		int n = sc.nextInt();

		System.out.print("N 입력");
		int x = sc.nextInt();
		int[] result = new int[n];
		for (int i = 0; i < n; i++) {
			System.out.print((i + 1) + "번째 정수 입력 >> ");
			result[i] = sc.nextInt();
		}

	}

}
