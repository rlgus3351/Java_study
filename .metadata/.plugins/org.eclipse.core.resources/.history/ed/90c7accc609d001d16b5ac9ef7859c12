package test2;

import java.util.Scanner;

public class Ex34 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
//		System.out.print("2진수를 입력하세요.");
		String str = "01001101";
		String[] temp = str.split("");
		
	}
	public static int powerN(int base, int n) {
		int result = base;
		for(int i=1; i<n; i++) {
			result *= base;
		}
		return result;
	}

}
