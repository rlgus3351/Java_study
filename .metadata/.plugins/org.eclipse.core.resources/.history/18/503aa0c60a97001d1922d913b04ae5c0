package javaproject;

import java.util.Random;

public class Ex21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 문제. 중복이 없이 숫자를 뽑는 로또 프로그램을 만드시오
		// 로또 번호 : 1~50
		Random r = new Random();
		
		int num = r.nextInt(50)+1;
		System.out.print("행운의 숫자 : " + num);

	}

}
