package test2;

import java.util.Random;

public class Ex21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random r = new Random();
		int[] array = new int[6];
		int[] temp = new int[6];
		for (int i = 0; i < array.length; i++) {
			array[i] = r.nextInt(50) + 1;
			if (i > 1 && array[i - 1] == array[i]) {
				i--;
			}

		}

	}

}
