package test2;

import java.util.Scanner;

public class Ex37 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("입력 : ");
		int num = sc.nextInt();
		int j = 1;
		int result = 0;
		for (int i = 1; i <= num; i++) {
			j *=  i;
		}
		System.out.println(result);
	}

}
