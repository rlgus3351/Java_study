package test1;

import java.util.Scanner;

public class Ex09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자 입력 :");
		int num = sc.nextInt();

		int round = 0;

		if (num % 10 > 5) {
			round = num / 10 * 10 + 10;
		} else {
			round = num / 10 * 10;
		}

		System.out.println("반올림수 : " + round);
	}

}
