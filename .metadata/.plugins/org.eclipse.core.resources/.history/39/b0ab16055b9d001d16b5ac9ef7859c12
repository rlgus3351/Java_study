package test2;

import java.util.Random;

public class Ex25 {

	public static void main(String[] args) {

		Random r = new Random();
		int[] point = { 92, 32, 52, 9, 81, 2, 68 };
		int num1 = r.nextInt(7) + 1;
		int num2 = r.nextInt(7) + 1;
		int distance = 0;
		int min = 0;
		if (num1 != num2) {
			distance = num1 > num2 ? num1 - num2 : num2 - num1;
			if (num1 > num2) {
				min = point[num2];
				for (int i = num2; i <= num1; i++) {
					if (min > point[i]) {
						min = point[i];
					}

				}
			}
			if (num2 > num1) {
				min = point[num2];
				for (int i = num1; i <= num2; i++) {
					if (min > point[i]) {
						min = point[i];
					}
				}
				result[1] = min;
			}

		}
	}

}
